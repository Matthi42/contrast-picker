{
  "applicationId": "js.neutralino.contrast-picker",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/vue/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,

  "nativeAllowList": [
    "app.*",
    "os.open",
    "storage.*",
    "extensions.*",
    "events.*"
  ],
  "modes": {
    "window": {
      "title": "Contrast Picker",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/vue/public/logo.png",
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "contrast-picker",
    "resourcesPath": "/vue/dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/vue/dist/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0",
    "frontendLibrary": {
      "patchFile": "/vue/index.html",
      "devUrl": "http://localhost:5173"
    }
  },
  "enableExtensions": true,
  "extensions": [
    {
      "id": "js.neutralino.adc",
      "commandLinux": "python3 ${NL_PATH}/extensions/adc/main.py",
      "commandWindows": "python ${NL_PATH}/extensions/adc/main.py"
    }
  ]
  
}